<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <!-- KEY 와 VALUE 가 필요함 -->
    <entry key="selectByFamilyName">
        <!-- VALUE 들어갈 자리 -->
        SELECT * FROM EMPLOYEE WHERE EMP_NAME LIKE CONCAT(?, '%')
    </entry>

</properties>